<p id="notice"><%= notice %></p>

<h1>Listing Civil Subjects</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Credit</th>
      <th>Code</th>
      <th>Category</th>
      <th>Semester</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @civil_subjects.each do |civil_subject| %>
      <tr>
        <td><%= civil_subject.name %></td>
        <td><%= civil_subject.credit %></td>
        <td><%= civil_subject.code %></td>
        <td><%= civil_subject.category %></td>
        <td><%= civil_subject.semester %></td>
        <td><%= link_to 'Show', civil_subject %></td>
        <td><%= link_to 'Edit', edit_civil_subject_path(civil_subject) %></td>
        <td><%= link_to 'Destroy', civil_subject, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Civil subject', new_civil_subject_path %>
