<div class="panel-body">
      <table class="table table-bordered table-hover table-striped">
        <thead>
          <tr>
            <th>Kode</th>
            <th>Nama</th>
            <th>SKS</th>
            <th>Semester</th>
            <th>Kategori</th>

          </tr>
        </thead>

        <tbody>
          <% @civil_subjects.each do |civil_subject| %>
            <tr>
              <td><%= civil_subject.code.upcase %></td>
              <td><%= civil_subject.name.titleize if civil_subject %></td>
              <td><%= civil_subject.credit %></td>
              <td><%= civil_subject.semester.name %></td>
              <td><%= civil_subject.category %></td>

            </tr>
          <% end %>
        </tbody>
      </table>

      <br>
    </div>

  </div>
