
<h3 class="lead">Struktural ISTPI</h1>

<table class="table table-stripped table-hover">
  <thead>
    <tr>
      <th>Nama</th>
      <th>Jabatan</th>
      <th>NIP</th>
      <th>NIDN</th>
    </tr>
  </thead>

  <tbody>
    <% @functional_structurals.each do |functional_structural| %>
      <tr>
        <td>
          <% if functional_structural.lecture.present? %>
            <%= link_to functional_structural.lecture.name, functional_structural %>
          <% else %>
            <%= link_to functional_structural.name_to_list, functional_structural %>
          <% end %>
        </td>
        <td><%= functional_structural.jabatan %></td>
        <td><%= functional_structural.nip %></td>
        <td><%= functional_structural.nidn %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
