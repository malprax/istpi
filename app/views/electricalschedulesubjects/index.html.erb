<% if @electricalschedulesubjects.present? %>
<h1 class="lead">Jadwal Kuliah Teknik Elektro</h1>

<table class="table table-hover table-striped">
  <thead>
    <tr>
      <th>Hari</th>
      <th>Waktu</th>
      <th>Mata Kuliah</th>
      <th>Dosen</th>
      <th>Ruangan</th>
      <th colspan="1"></th>
    </tr>
  </thead>

  <tbody>
    <% @electricalschedulesubjects.each do |electricalschedulesubject| %>
      <tr>
        <td><%= electricalschedulesubject.day rescue nil %></td>
        <td><%= electricalschedulesubject.classtime.time rescue nil %></td>
        <td><%= electricalschedulesubject.electrical_subject.name.titleize rescue nil %></td>
        <td><%= electricalschedulesubject.lecture1.name rescue nil %></td>
        <td><%= electricalschedulesubject.classroom.room.upcase rescue nil  %></td>
        <td><%= link_to 'Edit', edit_admin_electricalschedulesubject_path(electricalschedulesubject), class:'btn btn-warning' %>&nbsp;&nbsp;<%= link_to 'Hapus', admin_electricalschedulesubject_path(electricalschedulesubject), method: :delete, data: { confirm: 'Are you sure?' }, class:'btn btn-danger' %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<% else %>
<h3 class="lead">Maaf, Belum Ada Data Buku Induk Silahkan Kembali Lain Waktu..</h3>
<% end %>
