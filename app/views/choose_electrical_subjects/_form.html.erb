<%= simple_form_for(@choose_electrical_subject) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%#= f.input :electrical_subject_id %>
    <%= f.input :studiyear_id %>
    <%= f.input :user_id %>
    <%= f.association :electrical_subjects, as: :check_boxes %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
