<div class="well">
  <h3 class="lead">Daftar Pengajar ISTPI&nbsp;&nbsp;<%= link_to 'Buat', admin_new_dosen_path, class: "btn btn-primary" %><br></h3>
  <div class="panel panel-default">
    <div class="panel-title pull-right">
      <%= render 'shared/search' %>
    </div>
    <div class="panel-body">
      <table class="table table-striped table-hover">
        <thead>
          <tr>
            <th>Nama</th>
            <th>NIDN</th>
            <th>Fakultas</th>
            <th>Jurusan</th>
            <th>Alamat</th>
            <th>No kontak</th>
            <th colspan="2"></th>
          </tr>
        </thead>

        <tbody>
          <% @lectures.each do |lecture| %>
            <tr>
              <td><%= link_to lecture.name, admin_dosen_path(lecture) %></td>
              <td><%= lecture.nidn %></td>
              <td><%= lecture.fakultas %></td>
              <td><%= lecture.jurusan %></td>
              <td><%= lecture.alamat %></td>
              <td><%= lecture.no_kontak %></td>
              <td><%= link_to 'Edit', admin_edit_dosen_path(lecture) %></td>
              <td><%= link_to 'Hapus', admin_delete_dosen_path(lecture), method: :delete, data: { confirm: 'Are you sure?' } %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
        <%= will_paginate @lectures, :previous_label => '&laquo;', :next_label => '&raquo;', renderer: BootstrapPagination::Rails %>
    </div>
  </div>


  <%#= will_paginate @lectures %>
</div>
