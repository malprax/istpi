<% if @students_biographies.present? %>
<h3 class="lead">Daftar Nama Mahasiswa ISTPI Aktif&nbsp;&nbsp;<%= link_to 'Buat', new_students_biography_path, class: "btn btn-success" %>
</h3>

<table class="table table-hover table-striped">
  <thead>
    <tr>
      <th>Nama</th>
      <th>Stambuk</th>
      <th>Jenis kelamin</th>
      <th>Tempat, Tanggal Lahir</th>
      <th>Fakultas</th>
      <th>Jurusan</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @students_biographies.each do |students_biography| %>
      <tr>
        <td><%= link_to students_biography.user, students_biography %></td>
        <td><%= students_biography.stambuk %></td>
        <td><%= students_biography.jenis_kelamin %></td>
        <td><%= students_biography.tempat_lahir %>&nbsp; <%= students_biography.tanggal_lahir %></td>
        <td><%= students_biography.fakultas %></td>
        <td><%= students_biography.jurusan %></td>
        <td><%= link_to 'Edit', edit_admin_students_biography_path(students_biography) %></td>
        <td><%= link_to 'Hapus', admin_students_biography_path(students_biography), method: :delete, data: { confirm: 'Anda yakin?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<% else %>
<h3 class="lead">Maaf, Belum Ada Data Mahasiswa Silahkan Kembali Lain Waktu..</h3>
<% end %>
