<h1 class="lead">Daftar Waktu Kuliah</h1>

<table class="table table-hover table-striped">
  <thead>
    <tr>
      <th>Waktu Kuliah</th>
      <th colspan="1"></th>
    </tr>
  </thead>

  <tbody>
    <% @classtimes.each do |classtime| %>
      <tr>
        <td><%= classtime.time %></td>
        <td><%= link_to 'Edit', edit_admin_classtime_path(classtime), class: 'btn btn-warning' %>&nbsp;&nbsp;<%= link_to 'Hapus', admin_classtime_path(classtime), method: :delete, data: { confirm: 'Are you sure?' }, class:'btn btn-danger' %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Buat Waktu Kuliah', new_admin_classtime_path, class: 'btn btn-default' %>
