        <td><%= civilschedulesubject.count %></td>
        <td><%= link_to 'Show', civilschedulesubject %></td>
        <td><%= link_to 'Edit', edit_civilschedulesubject_path(civilschedulesubject) %></td>
        <td><%= link_to 'Destroy', civilschedulesubject, method: :delete, data: { confirm: 'Are you sure?' } %></td>
