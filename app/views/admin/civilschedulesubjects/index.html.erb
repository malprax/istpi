<h1>Listing Civilschedulesubjects</h1>

<table>
  <thead>
    <tr>
      <th>Civil subject</th>
      <th>Lecture1</th>
      <th>Lecture2</th>
      <th>Lecture3</th>
      <th>Day</th>
      <th>Time</th>
      <th>Year</th>
      <th>Room</th>
      <th>Count</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @civilschedulesubjects.each do |civilschedulesubject| %>
      <tr>
        <td><%= civilschedulesubject.civil_subject_id %></td>
