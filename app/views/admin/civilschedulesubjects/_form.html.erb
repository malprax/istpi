<%= simple_form_for([:admin,@civilschedulesubject]) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :civil_subject %>
    <%= f.input :lecture1_id %>
    <%= f.input :lecture2_id %>
    <%= f.input :lecture3_id %>
    <%= f.input :day %>
    <%= f.association :clock %>
    <%= f.association :year %>
    <%= f.association :room %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, class:'btn btn-default' %>&nbsp;&nbsp;<%= link_to 'Kembali', civilschedulesubjects_path, class:'btn btn-success' %>
  </div>
<% end %>
