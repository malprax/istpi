<p id="notice"><%= notice %></p>

<h3 class="lead">Daftar Calon Mahasiswa Baru</h3>

<table class= "table table-hover table-striped">
  <thead>
    <tr>
      <th>Nama</th>
      <th>Tempat lahir</th>
      <th>Tanggal lahir</th>
      <th>Jenis kelamin</th>
      <th>Alamat</th>
      <th>Kontak person</th>
      <th>Asal sekolah</th>
      <th>Tahun lulus</th>
      <th>Fakultas</th>
      <th>Jurusan</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @registrations.each do |registration| %>
      <tr>
        <td><%= link_to registration.nama, registration %></td>
        <td><%= registration.tempat_lahir %></td>
        <td><%= registration.tanggal_lahir %></td>
        <td><%= registration.jenis_kelamin %></td>
        <td><%= registration.alamat %></td>
        <td><%= registration.kontak_person %></td>
        <td><%= registration.asal_sekolah %></td>
        <td><%= registration.tahun_lulus %></td>
        <td><%= registration.fakultas_peminat %></td>
        <td><%= registration.jurusan_peminat %></td>

        <td><%= link_to 'Edit', edit_registration_path(registration) %></td>
        <td><%= link_to 'Destroy', registration, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Registration', new_registration_path, class: "btn btn-success" %>
