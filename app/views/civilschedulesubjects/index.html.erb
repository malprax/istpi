<% if @civilschedulesubjects.present? %>
<h3 class="lead">Jadwal Kuliah Teknik Sipil</h1>

<table class="table table-hover table-striped">
  <thead>
    <tr>
      <th>Day</th>
      <th>Time</th>
      <th>Mata Kuliah</th>
      <th>Room</th>
      <th>Dosen</th>
      <th colspan="1"></th>
    </tr>
  </thead>

  <tbody>
    <% @civilschedulesubjects.each do |civilschedulesubject| %>
      <tr>
        <td><%= civilschedulesubject.day %></td>
        <td><%= civilschedulesubject.time_id %></td>
        <td><%= civilschedulesubject.civil_subject_id %></td>
        <td><%= civilschedulesubject.room_id %></td>
        <td><%= civilschedulesubject.lecture1_id %></td>
        <td><%= link_to 'Edit', edit_civilschedulesubject_path(civilschedulesubject, class: 'btn btn-warning') %>&nbsp;&nbsp;<%= link_to 'Hapus', civilschedulesubject, method: :delete, data: { confirm: 'Are you sure?' },class:'btn btn-danger' %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<% else %>
<h3 class="lead">Maaf, Belum Ada Data Buku Induk Silahkan Kembali Lain Waktu..</h3>
<% end %>
